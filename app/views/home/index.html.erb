<h1>Current Accounts</h1>	

<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>Kind</th>
      <th>Expense</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.kind %></td>
        <td><%= account.expense %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account', new_account_path %>